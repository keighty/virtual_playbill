{
  "name": "testvp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "setup": "node ./config/schema.js; node ./config/seeds.js",
    "build": "webpack --progress --colors --watch",
    "test": "mocha --recursive test/server",
    "testWatch": "multi='mocha-osx-reporter=- spec=-' mocha --reporter mocha-multi --watch --recursive test/server",
    "testClient": "node_modules/karma/bin/karma start --reporters dots",
    "cover": "node_modules/istanbul/lib/cli.js cover _mocha -- --recursive test/server",
    "precommit": "eslint .; mocha --recursive test/server"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "mysql": "2.10.2",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "chai": "^3.5.0",
    "husky": "0.11.2",
    "istanbul": "^0.4.2",
    "karma-notification-reporter": "0.1.1",
    "mocha": "^2.4.5",
    "mocha-multi": "0.9.0",
    "mocha-osx-reporter": "0.1.2",
    "sinon": "^1.17.3",
    "webpack": "1.12.12",
    "webpack-dev-server": "1.12.1"
  }
}
